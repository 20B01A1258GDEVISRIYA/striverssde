 public static int reversePairs(ArrayList<Integer> arr) 
    {
        int cnt=0,n=arr.size();
        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++){
                if(arr.get(i)>2*arr.get(j)){
                    cnt++;
                }
            }
        }
        return cnt;
    }
